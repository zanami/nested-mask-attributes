{
  "name": "nested-mask-attributes",
  "version": "1.1.1",
  "description": "Library to hide or mask specific nested attributes that you desire from a javascript object",
  "main": "build/index.js",
  "scripts": {
    "build": "rimraf ./build && ./node_modules/typescript/bin/tsc -b --verbose",
    "prepublish": "npm run build",
    "lint": "eslint '*/**/*.{js,ts}'",
    "lint:fix": "eslint '*/**/*.{js,ts}' --fix",
    "test": "jest --passWithNoTests --maxWorkers=2 --config=./jest.config.js",
    "playground": "ts-node-dev ./lib/playground.ts"
  },
  "keywords": [
    "attribute",
    "object",
    "javascript"
  ],
  "author": "Rodrigo Luque",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.1.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.4",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rluque8/nested-mask-attributes.git"
  },
  "bugs": {
    "url": "https://github.com/rluque8/nested-mask-attributes/issues"
  }
}
